@startuml

interface CRM <<interface>>{
    RegisterNewClickCustomer()
    LogIn()
    GetClickCustomerData()
    ModifyClickCustomerData()
    SendNotifyToClickCustomer()
}

interface BookingSchedule <<interface>>{

    GetBookingSchedule()
    ModifyBookingSchedule()

}

interface QueueSchedule <<interface>>{

    GetQueueSchedule()
    ModifyQueueSchedule()
    GetMaxNumberInStore()
    ModifyMaxNumberInStore()

}

interface ManageMobileApp <<interface>>
interface ManageTicketMachine <<interface>>
interface ManageDigitalCounterpart <<interface>>
interface ManagementSystem <<interface>>
interface ManageQRCodeScannedMachine <<interface>>


CRM <|-- ManageMobileApp
QueueSchedule <|-- ManagementSystem
BookingSchedule <|-- ManagementSystem
QueueSchedule <|-- ManageTicketMachine
QueueSchedule <|-- ManageDigitalCounterpart
QueueSchedule <|-- ManageQRCodeScannedMachine


@enduml
